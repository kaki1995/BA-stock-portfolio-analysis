    "import numpy as np\n",
    "\n",
    "# Redefine trading days and import numpy\n",
    "trading_days_per_month = 21\n",
    "\n",
    "# Provided data\n",
    "daily_portfolio_returns = actual_portfolio_results_df['actual_portfolio_return'].values\n",
    "daily_portfolio_risks = actual_portfolio_results_df['actual_portfolio_risk'].values\n",
    "\n",
    "# Convert daily returns and risks to monthly returns and risks\n",
    "monthly_returns = [(1 + r) ** trading_days_per_month - 1 for r in daily_portfolio_returns]\n",
    "monthly_risks = [r * np.sqrt(trading_days_per_month) for r in daily_portfolio_risks]\n",
    "\n",
    "# Calculate total annual return by compounding the monthly returns\n",
    "annual_return = np.prod([1 + r for r in monthly_returns]) - 1\n",
    "\n",
    "# Calculate annualized risk by calculating the monthly risk and then annualizing it\n",
    "annualized_risk = np.std(monthly_returns) * np.sqrt(12)\n",
    "\n",
    "# Calculate Sharpe ratio\n",
    "risk_free_rate_annual = 0.042\n",
    "excess_return_annual = annual_return - risk_free_rate_annual\n",
    "sharpe_ratio_annual = excess_return_annual / annualized_risk if annualized_risk != 0 else np.nan\n",
    "\n",
    "# Prepare the final output results\n",
    "final_results = {\n",
    "    \"Annual Portfolio Return\": f\"{annual_return*100:.2f}%\",\n",
    "    \"Annual Portfolio Risk\": f\"{annualized_risk*100:.2f}%\",\n",
    "    \"Annual Sharpe Ratio\": f\"{sharpe_ratio_annual:.2f}\"\n",
    "}\n",
    "\n",
    "final_results\n"
   ]
