    "#SARIMAX\n",
    "def train_rolling_sarimax_multi(data, exog_vars, company_ids, window_size=wind, forecast_step=fc_step):\n",
    "    params = {}\n",
    "    predictions = {cid: [] for cid in company_ids}\n",
    "    true_values = {cid: [] for cid in company_ids}\n",
    "    mae_errors = {cid: [] for cid in company_ids}\n",
    "    rmse_errors = {cid: [] for cid in company_ids}\n",
    "    r2_scores = {cid: [] for cid in company_ids}\n",
    "\n",
    "    for cid in company_ids:\n",
    "        params[cid] = None\n",
    "\n",
    "    data = data.sort_values(by=['date', 'PERMNO'])\n",
    "\n",
    "    num_companies = len(company_ids)\n",
    "    total_iterations = (len(data) // num_companies) - window_size - forecast_step + 1\n",
    "\n",
    "    for start in range(total_iterations):\n",
    "        end = start + window_size\n",
    "        for cid in company_ids:\n",
    "            company_data = data[data['PERMNO'] == cid]\n",
    "            train_data = company_data.iloc[start:end]\n",
    "            test_data = company_data.iloc[end:end + forecast_step]\n",
    "\n",
    "            if len(train_data) == window_size and len(test_data) == forecast_step:\n",
    "                # Escalador ajustado solo para los datos de entrenamiento de RET\n",
    "                scaler = StandardScaler()\n",
    "                train_data['RET'] = scaler.fit_transform(train_data[['RET']])\n",
    "                test_data['RET'] = scaler.transform(test_data[['RET']])\n",
    "                \n",
    "                if params[cid] is not None:\n",
    "                    exog_train = train_data[exog_vars] if exog_vars else None\n",
    "                    exog_test = test_data[exog_vars] if exog_vars else None\n",
    "                    model = SARIMAX(train_data['RET'],\n",
    "                                    exog=exog_train,\n",
    "                                    order=params[cid]['order'],\n",
    "                                    seasonal_order=params[cid]['seasonal_order'])\n",
    "                    results = model.filter(params[cid]['params'])\n",
    "                else:\n",
    "                    exog_train = train_data[exog_vars] if exog_vars else None\n",
    "                    model = SARIMAX(train_data['RET'],\n",
    "                                    exog=exog_train,\n",
    "                                    order=(1, 1, 1),\n",
    "                                    seasonal_order=(1, 1, 1, 12))\n",
    "                    results = model.fit(disp=False)\n",
    "                    params[cid] = {'order': (1, 1, 1), 'seasonal_order': (1, 1, 1, 12), 'params': results.params}\n",
    "\n",
    "                exog_test = test_data[exog_vars] if exog_vars else None\n",
    "                if exog_test is not None and len(exog_test) >= forecast_step:\n",
    "                    pred_scaled = results.get_forecast(steps=forecast_step, exog=exog_test).predicted_mean\n",
    "                    pred_scaled = pred_scaled.to_numpy().reshape(-1, 1)  # Convertir a NumPy y reshape\n",
    "                    pred = scaler.inverse_transform(pred_scaled).flatten()\n",
    "                    predictions[cid].extend(pred)\n",
    "                    true_values[cid].extend(test_data['RET'].values)\n",
    "\n",
    "    # Calcular las métricas de error\n",
    "    avg_mae = 0\n",
    "    avg_rmse = 0\n",
    "    avg_r2 = 0\n",
    "    count = 0\n",
    "\n",
    "    for cid in company_ids:\n",
    "        if predictions[cid]:\n",
    "            all_preds = np.array(predictions[cid])\n",
    "            all_trues = np.array(true_values[cid])\n",
    "            mae = mean_absolute_error(all_trues, all_preds)\n",
    "            rmse = np.sqrt(mean_squared_error(all_trues, all_preds))\n",
    "            r2 = r2_score(all_trues, all_preds)\n",
    "            mae_errors[cid].append(mae)\n",
    "            rmse_errors[cid].append(rmse)\n",
    "            r2_scores[cid].append(r2)\n",
    "            avg_mae += mae\n",
    "            avg_rmse += rmse\n",
    "            avg_r2 += r2\n",
    "            count += 1\n",
    "\n",
    "    avg_mae /= count\n",
    "    avg_rmse /= count\n",
    "    avg_r2 /= count\n",
    "\n",
    "    return predictions, mae_errors, rmse_errors, r2_scores, avg_mae, avg_rmse, avg_r2\n",
    "\n",
    "def calculate_portfolio_performance(weights, mean_returns, cov_matrix):\n",
    "    returns = np.dot(weights, mean_returns)\n",
    "    std = np.sqrt(np.dot(weights.T, np.dot(cov_matrix, weights)))\n",
    "    return returns, std\n",
    "def scale_data(data, columns_to_scale):\n",
    "    scaler = MinMaxScaler()\n",
    "    data[columns_to_scale] = scaler.fit_transform(data[columns_to_scale])\n",
    "    return data, scaler\n",
    "def efficient_frontier(mean_returns, cov_matrix, risk_free_rate=0.03, num_portfolios=10000):\n",
    "    results = np.zeros((3, num_portfolios))\n",
    "    weights_record = []\n",
    "    for i in range(num_portfolios):\n",
    "        weights = np.random.random(len(mean_returns))\n",
    "        weights /= np.sum(weights)  # Normalize weights to sum to 1\n",
    "        weights_record.append(weights)\n",
    "        portfolio_return = np.dot(weights, mean_returns)\n",
    "        portfolio_std_dev = np.sqrt(np.dot(weights.T, np.dot(cov_matrix, weights)))\n",
    "        results[0, i] = portfolio_std_dev\n",
    "        results[1, i] = portfolio_return\n",
    "        results[2, i] = (portfolio_return - risk_free_rate) / portfolio_std_dev  # Sharpe Ratio\n",
    "\n",
    "    return results, weights_record\n",
    "\n",
    "def optimize_portfolio_gurobi(mean_returns, cov_matrix, risk_free_rate=0):\n",
    "    num_assets = len(mean_returns)\n",
    "    model = gp.Model(\"portfolio_optimization\")\n",
    "\n",
    "    # Variables: weights of the assets in the portfolio\n",
    "    weights = model.addVars(num_assets, lb=0, ub=1, name='weights')\n",
    "\n",
    "    # Calculate portfolio return\n",
    "    portfolio_return = gp.quicksum(weights[i] * mean_returns[i] for i in range(num_assets))\n",
    "\n",
    "    # Calculate portfolio variance\n",
    "    portfolio_variance = gp.quicksum(weights[i] * weights[j] * cov_matrix[i][j] \n",
    "                                     for i in range(num_assets) for j in range(num_assets))\n",
    "\n",
    "    # Introduce a new variable for the standard deviation\n",
    "    portfolio_std = model.addVar(lb=0, name='portfolio_std')\n",
    "\n",
    "    # Ensure that the square of the standard deviation equals the variance\n",
    "    model.addQConstr(portfolio_std * portfolio_std == portfolio_variance, name='std_deviation')\n",
    "\n",
    "    # Objective: maximize Sharpe ratio equivalent without division by a variable\n",
    "    model.setObjective((portfolio_return - risk_free_rate) * portfolio_std, GRB.MAXIMIZE)\n",
    "\n",
    "    # Constraint: weights must sum to 1\n",
    "    model.addConstr(gp.quicksum(weights[i] for i in range(num_assets)) == 1, \"budget\")\n",
    "\n",
    "    # Optimize the model\n",
    "    model.optimize()\n",
    "\n",
    "    # Retrieve optimized weights\n",
    "    optimized_weights = np.array([weights[i].X for i in range(num_assets)])\n",
    "    return optimized_weights\n",
    "\n",
    "def calculate_equal_weighted_return(mean_returns):\n",
    "    num_assets = len(mean_returns)\n",
    "    equal_weights = np.full(num_assets, 1 / num_assets)  # Pesos iguales\n",
    "    expected_portfolio_return = np.dot(equal_weights, mean_returns)\n",
    "    return expected_portfolio_return, equal_weights\n",
    "\n",
    "def main(data, exog_vars, company_ids, window_size=wind):\n",
    "    columns_to_scale = ['RET'] + exog_vars  # Incluye las columnas a escalar\n",
    "    data, scaler = scale_data(data, columns_to_scale)\n",
    "    predictions, mae_errors, rmse_errors, r2_scores, avg_mae, avg_rmse, avg_r2 = train_rolling_sarimax_multi(data, exog_vars, company_ids, window_size, forecast_step=21)\n",
    "    pred_df = pd.DataFrame(predictions)\n",
    "    \n",
    "    # Ensure all data is numeric\n",
    "    pred_df = pred_df.apply(pd.to_numeric, errors='coerce')\n",
    "\n",
    "    # Remove rows with NaNs in predictions\n",
    "    pred_df.dropna(inplace=True)\n",
    "    \n",
    "    # Expected returns and covariance matrix\n",
    "    mean_returns = pred_df.mean().values\n",
    "    cov_matrix = pred_df.cov().values\n",
    "\n",
    "    # Optimize portfolio using Gurobi\n",
    "    optimized_weights = optimize_portfolio_gurobi(mean_returns, cov_matrix)\n",
    "    print(f'Optimized weights: {optimized_weights}')\n",
    "\n",
    "    # Expected return and standard deviation of the optimized portfolio\n",
    "    expected_return, expected_std = calculate_portfolio_performance(optimized_weights, mean_returns, cov_matrix)\n",
    "    print(f'Expected portfolio return: {expected_return}')\n",
    "    print(f'Portfolio risk (standard deviation): {expected_std}')\n",
    "    \n",
    "    # Print error metrics\n",
    "    for cid in company_ids:\n",
    "        print(f'Company {cid} - MAE: {mae_errors[cid]}, RMSE: {rmse_errors[cid]}, R^2: {r2_scores[cid]}')\n",
    "\n",
    "    return optimized_weights, expected_return, expected_std\n",
    "\n",
    "company_ids = df['PERMNO'].unique()\n",
    "results = main(df, ['EXCHCD', 'DIVAMT', 'BIDLO', 'ASKHI', 'PRC', 'VOL', 'RET', 'SHROUT', \n",
    "                    'OPENPRC', 'NUMTRD', 'RETX', 'sprtrn', 'pe_exi', 'pe_inc', 'dpr', \n",
    "                    'npm', 'gpm', 'roa', 'roe', 'debt_at', 'dltt_be', 'intcov_ratio', \n",
    "                    'quick_ratio', 'curr_ratio', 'inv_turn', 'at_turn'], company_ids, window_size=wind)"
   ]
